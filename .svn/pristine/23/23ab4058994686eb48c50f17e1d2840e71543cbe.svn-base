.carousel-inner img{
    width: 70%;
    margin-left:auto; 
    margin-right:auto; 
    display:block; 
}
.carousel-caption{
    color: rgb(255, 51, 0);
    /* background-color: rgba(153, 204, 255, 0.7); */
    text-shadow: 5px 5px 20px #000 ;
    display: inline;
   
}
/*Media Query*/
@media (max-width: 992px){
	.carousel-caption {
		bottom: 15%;
	}
	.carousel-caption h1{
		font-size: 200%;
	}
	
}
@media (max-width: 768px){	
	.carousel-caption {
		bottom: 10%;
	}
	.carousel-caption h1{
		font-size: 170%;
	}
	
}
@media (max-width: 576px){
	.carousel-caption {
		bottom: 5%;
	}
	.carousel-caption h1{
		font-size: 130%;
	}
	
}

